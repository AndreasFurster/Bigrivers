    <div id="content">
        <table>
            <thead>
                <tr>
                    <th>Pagina</th>
                    <th>Auteur</th>
                    <th>Status</th>
                    <th>Updated</th>
                    <th>Acties</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>Page</td>
                    <td>Author</td>
                    <td>Gepubliceerd></td>
                    <td>Gewijzigd</td>
                    <td>@Html.ActionLink("Wijzig", "Edit", "Page") | @Html.ActionLink("Verwijder", "Delete", "Page")</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
